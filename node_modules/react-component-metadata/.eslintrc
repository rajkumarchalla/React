{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "constructor-super": 2,
    "comma-spacing": 2,
    "comma-dangle": 0,
    "semi-spacing": 0,
    "no-this-before-super": 2,
    "dot-notation": [2, { "allowKeywords": true }],
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "quotes": [2, "single", "avoid-escape"],
    "no-trailing-spaces": 1,
    "no-cond-assign": 0,
    "semi": [0, "never"],
    "curly": 0,
    "eol-last": 0,
    "no-multi-spaces": 0,
    "no-unused-expressions": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "key-spacing": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-sequences": 0,
    "no-return-assign": 0,
    "strict": 0
  }
}
